        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#ProductProperty"/>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#UvFilter -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#UvFilter">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Active"/>
        <rdfs:isDefinedBy xml:lang="en">Ingredients that protect the skin from sun damage and prevent the risks of skin cancer such as melanoma as well as sunburn, premature aging of the skin, or the appearance of spots. They can be mineral (ex. Titanium oxide, zinc oxide) or they can be organic (ex. Octocrylene).</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Vegetable_oil_emollient -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Vegetable_oil_emollient">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Emollient"/>
        <rdfs:isDefinedBy xml:lang="en">Sub type of emollient ingredient that are vegetable oil.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Viscosity -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Viscosity">
