        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#ThickenerProperty"/>
        <rdfs:isDefinedBy xml:lang="en">Stability with other ingredients because some rheological modifiers may cause the precipitation of other ingredients in the formula.</rdfs:isDefinedBy>
    </owl:Class>
    


