    <!-- https://purl.org/ontocosmetic#A1 -->


    <owl:NamedIndividual rdf:about="https://purl.org/ontocosmetic#A1">
        <rdf:type rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <hasHeuristicIngProp rdf:resource="https://purl.org/ontocosmetic#HLB"/>
        <hasHeuristicProdProp rdf:resource="https://purl.org/ontocosmetic#Stable"/>
