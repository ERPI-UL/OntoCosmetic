    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://purl.org/ontocosmetic#hasAfterfeeling -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasAfterfeeling">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Emollient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Afterfeeling"/>
        <rdfs:isDefinedBy xml:lang="en">Give one or several afterfeeling properties to an ingredient of type emollient.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasChemicalNature -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasChemicalNature">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Chemical_nature"/>
        <rdfs:isDefinedBy xml:lang="en">Give the chemical nature of an ingredient.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasDosage -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasDosage">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Formulation"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Dosage"/>
        <rdfs:isDefinedBy xml:lang="en">Give the dosages that compose a formulation.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasDosageIngredient -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasDosageIngredient">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Dosage"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:isDefinedBy xml:lang="en">Give the ingredient of a dosage.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasEmollience -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasEmollience">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Emollient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Emollience"/>
        <rdfs:isDefinedBy xml:lang="en">Give the level of emollience of an ingredient of type emollient.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


