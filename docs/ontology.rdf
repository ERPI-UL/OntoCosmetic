        <rdfs:isDefinedBy xml:lang="en">Associate an heuristic to the ingredient property that is involved in the rule.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicIngType -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicIngType">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Ingredient_Type"/>
