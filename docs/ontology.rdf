    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#O/W_emulsion -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#O/W_emulsion">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Microstructure"/>
        <owl:disjointWith rdf:resource="https://purl.org/ontocosmetic#W/O_emulsion"/>
        <rdfs:isDefinedBy xml:lang="en">It is an emulsion where the oil phase corresponds to the disperse phase and the continuous phase corresponds to the aqueous phase.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">oil-in-water emulsion</rdfs:label>
    </owl:Class>
    


