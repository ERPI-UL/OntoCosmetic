        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Ionic_Nature"/>
        <rdfs:isDefinedBy xml:lang="en">Define the ionic nature of an ingredient.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasOrigin -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasOrigin">
