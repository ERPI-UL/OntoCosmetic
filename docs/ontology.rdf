    <!-- https://purl.org/ontocosmetic#Surfactant -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Surfactant">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:isDefinedBy xml:lang="en">They are compounds that lower the interfacial tension between two immiscible phases. Surfactants are used in emulsions for four purposes: to decrease the energy required during emulsification, to adsorb rapidly at the interface of recently formed droplets, and to generate a barrier, either electrostatic, steric, or both, for long-term emulsion stability. Additionally, they can be also used as active ingredients in soaps, shampoos and conditioners. They can be classified according to their electrical nature in nonionic, ionic-anionic and ionic-cationic. They can be classified according to their chemical nature.</rdfs:isDefinedBy>
        <rdfs:seeAlso xml:lang="en">emulsifier</rdfs:seeAlso>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#SurfactantProperty -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#SurfactantProperty">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Property"/>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Surfactant_Dosage -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Surfactant_Dosage">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/ontocosmetic#Dosage"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.org/ontocosmetic#hasDosageIngredient"/>
                        <owl:someValuesFrom rdf:resource="https://purl.org/ontocosmetic#Surfactant"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Dosage"/>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Suspension -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Suspension">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#HeterogenousMixture"/>
        <rdfs:isDefinedBy xml:lang="en">It is cloudy and heterogeneous. The particles are larger than 10,000 Angstroms which allows them to be filtered. If a suspension is allowed to stand the particles will separate out.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#System -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#System">
        <rdfs:isDefinedBy xml:lang="en">Anything can be described as a system. (Substance, process, product, mixture, etc). It is a superclass of substance and process.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Texture -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Texture">
