     xmlns:ontocosmetic="https://purl.org/ontocosmetic#">
    <owl:Ontology rdf:about="https://purl.org/ontocosmetic">
        <rdfs:comment xml:lang="en">An ontology that describes chemical formulation in the perspective to assist the design process of cosmetic product.</rdfs:comment>
        <rdfs:label xml:lang="en">Onto Cosmetic</rdfs:label>
        <owl:priorVersion xml:lang="en">2.0</owl:priorVersion>
        <owl:versionInfo xml:lang="en">3.0</owl:versionInfo>
    </owl:Ontology>
    


