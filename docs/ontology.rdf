        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#DropletSize"/>
        <rdfs:isDefinedBy xml:lang="en">Associate an heuristic to the structure property of the formulation final product  that is involved in the rule.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProcessProp -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProcessProp">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Process"/>
        <rdfs:isDefinedBy xml:lang="en">Associate an heuristic to the process used to do the formulation final product. This process will have an impact on the product structure or properties.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProdProp -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProdProp">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#ProductProperty"/>
        <rdfs:isDefinedBy xml:lang="en">Associate an heuristic to a product property. The rule has an impact on the properties of the final product formulated.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


