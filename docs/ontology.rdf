    <!-- https://purl.org/ontocosmetic#Thickener -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Thickener">
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:isDefinedBy xml:lang="en">It is a substance that can significantly modify the rheology of a fluid. Rheological modifiers are added to the formulation for three main purposes: to increase viscosity, to promote a particular rheological behavior (pseudo- plastic and thixotropic), and to enhance emulsion stability.</rdfs:isDefinedBy>
        <rdfs:seeAlso xml:lang="en">Rheological modifiers</rdfs:seeAlso>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#ThickenerProperty -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#ThickenerProperty">
