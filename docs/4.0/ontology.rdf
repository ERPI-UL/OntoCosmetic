    <owl:Class rdf:about="https://purl.org/ontocosmetic#SurfactantProperty">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/ontocosmetic#Property"/>
