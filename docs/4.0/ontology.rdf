        <rdfs:label xml:lang="en">has qualitative value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasSpreading -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasSpreading">
