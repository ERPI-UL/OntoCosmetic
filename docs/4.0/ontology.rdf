        <rdfs:label xml:lang="en">has emollience level</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicIngProp -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicIngProp">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Property"/>
        <rdfs:comment xml:lang="en">Associate an heuristic to the ingredient property that is involved in the rule.</rdfs:comment>
        <rdfs:label xml:lang="en">is affected by ingredient property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicIngType -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicIngType">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#ChemicalFamily"/>
        <rdfs:comment xml:lang="en">Define which ingredient type is concerned by the heuristic.</rdfs:comment>
        <rdfs:label xml:lang="en">involves ingredient type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProcessOutcome -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProcessOutcome">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#DropletSizeState"/>
        <rdfs:comment xml:lang="en">Associate a heuristic to the structure property of the formulation final product  that is involved in the rule.</rdfs:comment>
        <rdfs:label xml:lang="en">has structural property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProcessProp -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProcessProp">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#UnitOperation"/>
        <rdfs:comment xml:lang="en">Associate an heuristic to the process used to do the formulation final product. This process will have an impact on the product structure or properties.</rdfs:comment>
        <rdfs:label xml:lang="en">is produced through</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProdProp -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProdProp">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Property"/>
        <rdfs:comment xml:lang="en">Associate an heuristic to a product property. The rule has an impact on the properties of the final product formulated.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasHeuristicProductPropertyState -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasHeuristicProductPropertyState">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#PropertyState"/>
        <rdfs:comment xml:lang="en">Define the state that is given to the property when the heuristic is verified.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasIngredient -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasIngredient">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Formulation"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:comment xml:lang="en">Relationship that defines the ingredients that are used in a formulation.</rdfs:comment>
        <rdfs:label xml:lang="en">has ingredient</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasIngredientPropertyState -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasIngredientPropertyState">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Heuristic"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#PropertyState"/>
        <rdfs:comment xml:lang="en">Defines a relation between an heuristic and a specific state of a property. It defines the property state that could trigger the heuristic.</rdfs:comment>
        <rdfs:label xml:lang="en">has ingredient property state</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasIonicClass -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasIonicClass">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
