     -->


    


    <!-- https://purl.org/ontocosmetic#definesStateOf -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#definesStateOf">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#PropertyState"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Property"/>
        <rdfs:comment xml:lang="en">Relationship between qualitative state of substance property and the property.</rdfs:comment>
        <rdfs:label xml:lang="en">defines state of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasAfterfeeling -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasAfterfeeling">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Emollient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#AfterfeelingState"/>
        <rdfs:comment xml:lang="en">Give one or several afterfeeling properties to an ingredient of type emollient.</rdfs:comment>
        <rdfs:label xml:lang="en">has afterfeeling</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasChemicalNature -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasChemicalNature">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Chemical_nature"/>
        <rdfs:comment xml:lang="en">Give the chemical nature of an ingredient.</rdfs:comment>
        <rdfs:label xml:lang="en">has chemical nature</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasDosage -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasDosage">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Formulation"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Dosage"/>
        <rdfs:comment xml:lang="en">Give the dosages that composes a formulation.</rdfs:comment>
        <rdfs:label xml:lang="en">has dosage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasEmollience -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasEmollience">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Emollient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#EmollienceState"/>
