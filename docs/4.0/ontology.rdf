        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#IonicState"/>
        <rdfs:comment xml:lang="en">Define the ionic nature of an ingredient.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasOrigin -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasOrigin">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Origin"/>
        <rdfs:comment xml:lang="en">Provide the origin of an ingredient</rdfs:comment>
        <rdfs:label xml:lang="en">has origin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasPhase -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasPhase">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Ingredient"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#PhaseState"/>
        <rdfs:comment xml:lang="en">it define which phase the ingredient is soluble in.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasProcess -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasProcess">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Formulation"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#Process"/>
        <rdfs:comment xml:lang="en">Processing done on the ingredient of the formulation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://purl.org/ontocosmetic#hasProductDropletSize -->


    <owl:ObjectProperty rdf:about="https://purl.org/ontocosmetic#hasProductDropletSize">
        <rdfs:domain rdf:resource="https://purl.org/ontocosmetic#Formulation"/>
        <rdfs:range rdf:resource="https://purl.org/ontocosmetic#DropletSizeState"/>
