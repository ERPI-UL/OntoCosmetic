    <owl:Class rdf:about="https://purl.org/ontocosmetic#ThickenerProperty">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/ontocosmetic#Property"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.org/ontocosmetic#isPropertyOf"/>
