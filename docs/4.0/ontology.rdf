                        <owl:onProperty rdf:resource="https://purl.org/ontocosmetic#isPropertyOf"/>
                        <owl:hasValue rdf:resource="https://purl.org/ontocosmetic#surfactant_type"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Property"/>
    </owl:Class>
    


    <!-- https://purl.org/ontocosmetic#Surfactant_Dosage -->


    <owl:Class rdf:about="https://purl.org/ontocosmetic#Surfactant_Dosage">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purl.org/ontocosmetic#Dosage"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://purl.org/ontocosmetic#isQuantifying"/>
                        <owl:someValuesFrom rdf:resource="https://purl.org/ontocosmetic#Surfactant"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://purl.org/ontocosmetic#Dosage"/>
        <rdfs:comment xml:lang="en">Sub-type of dosage which concerns only the surfactant.</rdfs:comment>
        <rdfs:label xml:lang="en">Surfactant dosage</rdfs:label>
    </owl:Class>
    


