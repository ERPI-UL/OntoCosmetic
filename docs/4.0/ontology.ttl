<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://purl.org/ontocosmetic#definesStateOf
:definesStateOf rdf:type owl:ObjectProperty ;
                rdfs:domain :PropertyState ;
                rdfs:range :Property ;
                rdfs:comment "Relationship between qualitative state of substance property and the property."@en ;
                rdfs:label "defines state of"@en .


###  https://purl.org/ontocosmetic#hasAfterfeeling
:hasAfterfeeling rdf:type owl:ObjectProperty ;
                 rdfs:domain :Emollient ;
                 rdfs:range :AfterfeelingState ;
                 rdfs:comment "Give one or several afterfeeling properties to an ingredient of type emollient."@en ;
                 rdfs:label "has afterfeeling"@en .


###  https://purl.org/ontocosmetic#hasChemicalNature
:hasChemicalNature rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Ingredient ;
                   rdfs:range :Chemical_nature ;
                   rdfs:comment "Give the chemical nature of an ingredient."@en ;
                   rdfs:label "has chemical nature"@en .


###  https://purl.org/ontocosmetic#hasDosage
:hasDosage rdf:type owl:ObjectProperty ;
           rdfs:domain :Formulation ;
           rdfs:range :Dosage ;
           rdfs:comment "Give the dosages that compose a formulation."@en ;
           rdfs:label "has dosage"@en .


###  https://purl.org/ontocosmetic#hasEmollience
:hasEmollience rdf:type owl:ObjectProperty ;
               rdfs:domain :Emollient ;
               rdfs:range :EmollienceState ;
               rdfs:comment "Give the level of emollience of an ingredient of type emollient."@en ;
               rdfs:label "has emollience level"@en .


###  https://purl.org/ontocosmetic#hasHeuristicIngProp
:hasHeuristicIngProp rdf:type owl:ObjectProperty ;
                     rdfs:domain :Heuristic ;
                     rdfs:range :Property ;
                     rdfs:comment "Associate an heuristic to the ingredient property that is involved in the rule."@en ;
                     rdfs:label "is affected by ingredient property"@en .


###  https://purl.org/ontocosmetic#hasHeuristicIngType
:hasHeuristicIngType rdf:type owl:ObjectProperty ;
                     rdfs:domain :Heuristic ;
                     rdfs:range :ChemicalFamily ;
                     rdfs:comment "Define which ingredient type is concerned by the heuristic."@en ;
                     rdfs:label "involves ingredient type"@en .


###  https://purl.org/ontocosmetic#hasHeuristicProcessOutcome
:hasHeuristicProcessOutcome rdf:type owl:ObjectProperty ;
                            rdfs:domain :Heuristic ;
                            rdfs:range :DropletSizeState ;
                            rdfs:comment "Associate an heuristic to the structure property of the formulation final product  that is involved in the rule."@en ;
                            rdfs:label "has structural property"@en .


###  https://purl.org/ontocosmetic#hasHeuristicProcessProp
:hasHeuristicProcessProp rdf:type owl:ObjectProperty ;
                         rdfs:domain :Heuristic ;
                         rdfs:range :UnitOperation ;
                         rdfs:comment "Associate an heuristic to the process used to do the formulation final product. This process will have an impact on the product structure or properties."@en ;
                         rdfs:label "is produced through"@en .


###  https://purl.org/ontocosmetic#hasHeuristicProdProp
:hasHeuristicProdProp rdf:type owl:ObjectProperty ;
                      rdfs:domain :Heuristic ;
                      rdfs:range :Property ;
                      rdfs:comment "Associate an heuristic to a product property. The rule has an impact on the properties of the final product formulated."@en .


###  https://purl.org/ontocosmetic#hasHeuristicProductPropertyState
:hasHeuristicProductPropertyState rdf:type owl:ObjectProperty ;
                                  rdfs:domain :Heuristic ;
                                  rdfs:range :PropertyState ;
                                  rdfs:comment "Define the state that is given to the property when the heuristic is verified."@en .


###  https://purl.org/ontocosmetic#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:domain :Formulation ;
               rdfs:range :Ingredient ;
               rdfs:comment "Relationship that defines the ingredients that are used in a fomulation."@en ;
               rdfs:label "has ingredient"@en .


###  https://purl.org/ontocosmetic#hasIngredientPropertyState
:hasIngredientPropertyState rdf:type owl:ObjectProperty ;
                            rdfs:domain :Heuristic ;
                            rdfs:range :PropertyState ;
                            rdfs:comment "Defines a relation between an heuristic and a specific state of a property. It defines the property state that could trigger the heuristic."@en ;
                            rdfs:label "has ingredient property state"@en .


###  https://purl.org/ontocosmetic#hasIonicClass
:hasIonicClass rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Surfactant ;
               rdfs:range :IonicState ;
               rdfs:comment "Define the ionic nature of an ingredient."@en .


###  https://purl.org/ontocosmetic#hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
           rdfs:domain :Ingredient ;
           rdfs:range :Origin ;
           rdfs:comment "Provide the origin of an ingredient"@en ;
           rdfs:label "has origin"@en .


###  https://purl.org/ontocosmetic#hasPhase
:hasPhase rdf:type owl:ObjectProperty ;
          rdfs:domain :Ingredient ;
          rdfs:range :PhaseState ;
          rdfs:comment "it define which phase the ingredient is soluble in."@en .


###  https://purl.org/ontocosmetic#hasProcessUnit
:hasProcessUnit rdf:type owl:ObjectProperty ;
                rdfs:domain :Formulation ;
                rdfs:range :UnitOperation ;
                rdfs:comment "Processing done on the ingredient of the formulation."@en .


###  https://purl.org/ontocosmetic#hasProductDropletSize
:hasProductDropletSize rdf:type owl:ObjectProperty ;
                       rdfs:domain :Formulation ;
                       rdfs:range :DropletSizeState ;
                       rdfs:comment "Provides the size of the droplet of the final product of a formulation. This dorplet will depends on process."@en .


###  https://purl.org/ontocosmetic#hasProductOiliness
:hasProductOiliness rdf:type owl:ObjectProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range :Oiliness ;
                    rdfs:comment "Define the oiliness of formulated product."@en .


###  https://purl.org/ontocosmetic#hasProductStability
:hasProductStability rdf:type owl:ObjectProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range :StabilityState ;
                     rdfs:comment "Define the stability of a fomulation final product."@en .


###  https://purl.org/ontocosmetic#hasProductViscosity
:hasProductViscosity rdf:type owl:ObjectProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range :ViscosityState ;
                     rdfs:comment "Provides the viscosity of the formulation final product."@en .


###  https://purl.org/ontocosmetic#hasQualitativeValue
:hasQualitativeValue rdf:type owl:ObjectProperty ;
                     rdfs:domain :Property ;
                     rdfs:range :PropertyState ;
                     rdfs:comment "Relationship that link a property to the qualitative value that are associated to the property."@en ;
                     rdfs:label "has qualitative value"@en .


###  https://purl.org/ontocosmetic#hasSpreading
:hasSpreading rdf:type owl:ObjectProperty ;
              rdfs:domain :Emollient ;
              rdfs:range :SpreadingState ;
              rdfs:comment "Defines the spreading property of an emollient."@en .


###  https://purl.org/ontocosmetic#hasState
:hasState rdf:type owl:ObjectProperty ;
          rdfs:domain :Substance ;
          rdfs:range :StateOfMatter .


###  https://purl.org/ontocosmetic#hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            rdfs:domain :Surfactant ;
            rdfs:range :TextureState ;
            rdfs:comment "Provides the texture that can be expected from a given surfactant."@en .


###  https://purl.org/ontocosmetic#isCommercializedAs
:isCommercializedAs rdf:type owl:ObjectProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range :Product .


###  https://purl.org/ontocosmetic#isPropertyOf
:isPropertyOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Property ;
              rdfs:range :ChemicalFamily ;
              rdfs:comment "Define the type of substance (chemical nature) that is concerned by the property."@en ;
              rdfs:label "is property of"@en .


###  https://purl.org/ontocosmetic#isQuantifying
:isQuantifying rdf:type owl:ObjectProperty ;
               rdfs:domain :Dosage ;
               rdfs:range :Ingredient ;
               rdfs:comment "Give the ingredient of a dosage."@en ;
               rdfs:label "is quantifying"@en .


###  https://purl.org/ontocosmetic#isValidatedBy
:isValidatedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Formulation ;
               rdfs:range :Heuristic ;
               rdfs:comment "Defines that a formulation validate a given heuristic."@en .


#################################################################
#    Data properties
#################################################################

###  https://purl.org/ontocosmetic#hasCalculatedHLB
:hasCalculatedHLB rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Formulation ;
                  rdfs:range xsd:float ;
                  rdfs:comment "the final HLB of the surfactant mixture that compose the formulation"@en .


###  https://purl.org/ontocosmetic#hasEquivalentHLB
:hasEquivalentHLB rdf:type owl:DatatypeProperty .


###  https://purl.org/ontocosmetic#hasHLB
:hasHLB rdf:type owl:DatatypeProperty ;
        rdfs:domain :Surfactant ;
        rdfs:comment "the HLB that a surfactant can have"@en .


###  https://purl.org/ontocosmetic#hasHeuristicDescription
:hasHeuristicDescription rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Heuristic ;
                         rdfs:comment "Data properties that provides a description of the heuristic."@en .


###  https://purl.org/ontocosmetic#hasHeuristicHighThreshold
:hasHeuristicHighThreshold rdf:type owl:DatatypeProperty ;
                           rdfs:domain :Heuristic ;
                           rdfs:range xsd:float ;
                           rdfs:comment "High threshold in percentage for an heuristique."@en .


###  https://purl.org/ontocosmetic#hasHeuristicLowThreshold
:hasHeuristicLowThreshold rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Heuristic ;
                          rdfs:range xsd:float ;
                          rdfs:comment "Low threshold in percentage for an heuristique."@en .


###  https://purl.org/ontocosmetic#hasHeuristicSource
:hasHeuristicSource rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Heuristic ;
                    rdfs:comment "Provides the source of the heuristic (from literature or experience)"@en .


###  https://purl.org/ontocosmetic#hasINCIcode
:hasINCIcode rdf:type owl:DatatypeProperty ;
             rdfs:domain :Ingredient ;
             rdfs:range xsd:string ;
             rdfs:comment "the INCI code that a ingredient can have."@en .


###  https://purl.org/ontocosmetic#hasNbSurfactantIng
:hasNbSurfactantIng rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range xsd:int ;
                    rdfs:comment "Define the number of surfactants inside a given formulation."@en .


###  https://purl.org/ontocosmetic#hasPricePerKilogram
:hasPricePerKilogram rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Ingredient ;
                     rdfs:range xsd:float ;
                     rdfs:comment "Price per kilogram of an ingredient"@en .


###  https://purl.org/ontocosmetic#hasProductOilyPhase
:hasProductOilyPhase rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range xsd:float ;
                     rdfs:comment "Percentage of the formulated product that represent oily ingrdient."@en .


###  https://purl.org/ontocosmetic#hasQuantity
:hasQuantity rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Dosage ;
             rdfs:range xsd:float ;
             rdfs:comment "the quantity of ingredient that compose the dosage. This quantity is in percentage of volume of the formulation."@en .


###  https://purl.org/ontocosmetic#hasQuantitySum
:hasQuantitySum rdf:type owl:DatatypeProperty ;
                rdfs:domain :Dosage ;
                rdfs:range xsd:float ;
                rdfs:comment "the quantity sum is an intermediary value that allow the calculation of the total quantity of product in a formulation, except the water. This quantity sum will be used in the calculation of the water quantity."@en .


###  https://purl.org/ontocosmetic#hasRequiredHLB
:hasRequiredHLB rdf:type owl:DatatypeProperty ;
                rdfs:domain :Emollient ;
                rdfs:comment "the RHLB that an emollient can have."@en .


###  https://purl.org/ontocosmetic#hasSurfactantQuantitySum
:hasSurfactantQuantitySum rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Dosage ;
                          rdfs:range xsd:float ;
                          rdfs:comment "The surfactant quantity sum is an intermediary value that allow the calculation of the total quantity of surfactant in a formulation."@en .


###  https://purl.org/ontocosmetic#hasTotalOilyPhase
:hasTotalOilyPhase rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Formulation ;
                   rdfs:range xsd:float .


###  https://purl.org/ontocosmetic#hasTotalSurfactantQuantity
:hasTotalSurfactantQuantity rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain :Formulation ;
                            rdfs:range xsd:float ;
                            rdfs:comment "The total quantity of surfactant that a formulation have. This quantity is in percentage of volume."@en .


###  https://purl.org/ontocosmetic#hasTotalThickenerQuantity
:hasTotalThickenerQuantity rdf:type owl:DatatypeProperty ;
                           rdfs:domain :Formulation ;
                           rdfs:range xsd:float ;
                           rdfs:comment "The total quantity of thickener that a formulation have. This quantity is in percentage of volume."@en .


###  https://purl.org/ontocosmetic#isActive
:isActive rdf:type owl:DatatypeProperty ;
          rdfs:domain :Heuristic ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Property that allow to enable/ disable a heuristic"@en .


###  https://purl.org/ontocosmetic#isConform
:isConform rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Formulation ;
           rdfs:range xsd:boolean ;
           rdfs:comment "the data property idicates if a formulation is conform to the restricive heuristics that correspond to formulation specification."@en .


#################################################################
#    Classes
#################################################################

###  https://purl.org/ontocosmetic#AbsorptionRateState
:AbsorptionRateState rdf:type owl:Class ;
                     rdfs:subClassOf :FormulationPropertyState ;
                     rdfs:comment "Describe the capacity of the formulation product to be absopt by a skin."@en ;
                     rdfs:label "After Feel - Absorption Rate"@en .


###  https://purl.org/ontocosmetic#Active
:Active rdf:type owl:Class ;
        rdfs:subClassOf :Ingredient ;
        rdfs:comment "Ingredients that perform an specific function on customer skin. [cosmetic sector definition]. It is a chemical that helps with the main function of the cosmetic product."@en ;
        rdfs:label "Active"@en .


###  https://purl.org/ontocosmetic#Aerosol
:Aerosol rdf:type owl:Class ;
         rdfs:subClassOf :Colloid ;
         rdfs:comment "solid or liquid particles in a gas"@en .


###  https://purl.org/ontocosmetic#AfterfeelingState
:AfterfeelingState rdf:type owl:Class ;
                   rdfs:subClassOf :IngredientPropertyState ;
                   rdfs:comment "Ingredients have the property to influence the afterfeel of the formulated product."@en .


###  https://purl.org/ontocosmetic#AgitationState
:AgitationState rdf:type owl:Class ;
                rdfs:subClassOf :ProcessState ;
                rdfs:label "Agitatione state"@en ,
                           "Define the qualitative that the agitation process can have."@en .


###  https://purl.org/ontocosmetic#AmphotericSurfactant
:AmphotericSurfactant rdf:type owl:Class ;
                      rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#AnionicSurfactant
:AnionicSurfactant rdf:type owl:Class ;
                   rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#AnionicThickener
:AnionicThickener rdf:type owl:Class ;
                  rdfs:subClassOf :IonicThickener ;
                  rdfs:label "Anionic thickener"@en .


###  https://purl.org/ontocosmetic#Antioxidant
:Antioxidant rdf:type owl:Class ;
             rdfs:subClassOf :Active ;
             rdfs:comment "Chemicals that interrupt oxidation reactions and prevent the effects of oxygen radicals (e.g. peroxides) both processes known to damage the integrity and function of various natural substances. Antioxidants are useful in two ways: On the one hand they prevent degradation of natural ingredients (proteins, sugars, lipids) in the cosmetic product. On the other hand antioxidants protect the skin cells from being damaged and slow down the aging process. Antioxidants have been shown to boost the skin's radiance, minimize age spots, sun spots, and fine lines."@en .


###  https://purl.org/ontocosmetic#AqueousThickeners
:AqueousThickeners rdf:type owl:Class ;
                   rdfs:subClassOf :Thickener ;
                   rdfs:label "Aqueous Thickeners"@en .


###  https://purl.org/ontocosmetic#CationicSurfactant
:CationicSurfactant rdf:type owl:Class ;
                    rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#CationicThickener
:CationicThickener rdf:type owl:Class ;
                   rdfs:subClassOf :IonicThickener ;
                   rdfs:label "Cationic thickener"@en .


###  https://purl.org/ontocosmetic#ChelatingAgent
:ChelatingAgent rdf:type owl:Class ;
                rdfs:subClassOf :OtherIngredient ;
                rdfs:comment "A substance that reacts with metal ions to form stable, water-soluble metal. They are used in liquid soap and body wash to prevent fragrance and color degradation and for their synergistic effect with antimicrobials. The process of chelation stabilizes metal ions by preventing them from chemically reacting with any other substances."@en ;
                rdfs:label "Chelating agent"@en .


###  https://purl.org/ontocosmetic#ChemicalFamily
:ChemicalFamily rdf:type owl:Class ;
                rdfs:subClassOf :PropertyState .


###  https://purl.org/ontocosmetic#Chemical_nature
:Chemical_nature rdf:type owl:Class ;
                 rdfs:subClassOf :PropertyState ;
                 rdfs:comment "Classification that a substance can have"@en ;
                 rdfs:label "Chemical nature"@en .


###  https://purl.org/ontocosmetic#Colloid
:Colloid rdf:type owl:Class ;
         rdfs:subClassOf :HeterogenousMixture ;
         rdfs:comment "It is a heterogeneous mixture in which a substance with a microscopic structure of disperse insoluble particles (disperse phase) is suspended in another substance (continuous phase). It can exist naturally, or it can be technically formulated, and in that case, it can also be named as a Formulation."@en ;
         rdfs:seeAlso "colloidal system"@en .


###  https://purl.org/ontocosmetic#Dosage
:Dosage rdf:type owl:Class ;
        rdfs:comment "Dosages are the combination of an ingredient and a quantity. It constitute a part of a formulation."@en ;
        rdfs:label "Dosage"@en .


###  https://purl.org/ontocosmetic#DropletSizeState
:DropletSizeState rdf:type owl:Class ;
                  rdfs:subClassOf :StructurePropertyState ;
                  rdfs:label "Droplet size state"@en .


###  https://purl.org/ontocosmetic#EmollienceState
:EmollienceState rdf:type owl:Class ;
                 rdfs:subClassOf :PropertyState .


###  https://purl.org/ontocosmetic#Emollient
:Emollient rdf:type owl:Class ;
           rdfs:subClassOf :Ingredient ;
           rdfs:comment "Ingredient of oily nature used in cosmetics to make skin and hair softener. Some of them can serve as solvent for liposoluble actives. Additionally, some of them can act as occlusive ingredients, i.e., they create a barrier to avoid the loss of water. It can be classified according to its chemical nature into the sub-classes:  ester, fatty alcohol, fatty acid, hydrocarbon, silicon, triglyceride."@en ;
           rdfs:label "Emollient"@en .


###  https://purl.org/ontocosmetic#EmollientProperty
:EmollientProperty rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Property
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :isPropertyOf ;
                                                                owl:hasValue :emollient_type
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Property .


###  https://purl.org/ontocosmetic#Emollient_Dosage
:Emollient_Dosage rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Dosage
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :isQuantifying ;
                                                               owl:someValuesFrom :Emollient
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Dosage ;
                  owl:disjointWith :Surfactant_Dosage ;
                  rdfs:comment "Sub-type of dosage which concerns only the emollient."@en ;
                  rdfs:label "Emollient dosage"@en .


###  https://purl.org/ontocosmetic#Emulsion
:Emulsion rdf:type owl:Class ;
          rdfs:subClassOf :Colloid ;
          rdfs:comment "It is a heterogeneous mixture of two or more immiscible liquids. It is a sub-class of a more general class known as colloid."@en .


###  https://purl.org/ontocosmetic#EmulsionScienceHeuristic
:EmulsionScienceHeuristic rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( :Heuristic
                                                                     [ rdf:type owl:Class ;
                                                                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :hasHeuristicSource ;
                                                                                       owl:hasValue "Heuristic"@en
                                                                                     ]
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :hasHeuristicSource ;
                                                                                       owl:hasValue "Heuristic"
                                                                                     ]
                                                                                   )
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf :Heuristic ;
                          rdfs:comment "It is a sub-type of heuristic that come from scientific literature."@en .


###  https://purl.org/ontocosmetic#Ester_emollient
:Ester_emollient rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasChemicalNature ;
                                       owl:hasValue :ester
                                     ] ;
                 rdfs:subClassOf :Emollient ;
                 rdfs:comment "chemical compound derived from an oxoacid (organic or inorganic) in which at least one –OH hydroxyl group is replaced by an –O– alkyl (alkoxy) group."@en .


###  https://purl.org/ontocosmetic#Ether_emollient
:Ether_emollient rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasChemicalNature ;
                                       owl:hasValue :ether
                                     ] ;
                 rdfs:subClassOf :Emollient ;
                 rdfs:comment "Sub type of emollient ingredient that are ether."@en .


###  https://purl.org/ontocosmetic#ExpertKnowledgeHeuristic
:ExpertKnowledgeHeuristic rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( :Heuristic
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :hasHeuristicSource ;
                                                                       owl:hasValue "Matrix"
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] ;
                          rdfs:subClassOf :Heuristic ;
                          rdfs:comment "It is a sub-type of heuristic that come from expert  interview."@en .


