@prefix : <https://purl.org/ontocosmetic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://purl.org/ontocosmetic#> .

<https://purl.org/ontocosmetic> rdf:type owl:Ontology ;
                                 owl:versionIRI <https://purl.org/ontocosmetic/4.0> ;
                                 <http://purl.org/dc/elements/1.1/contributor> "" ,
                                                                               "Juliana Serna Rodas" ;
                                 <http://purl.org/dc/elements/1.1/creator> "Alex Gabriel" ;
                                 <http://purl.org/dc/elements/1.1/description> "An ontology that describes chemical formulation in the perspective to assist the design process of cosmetic product."@en ;
                                 <http://purl.org/dc/elements/1.1/title> "Onto Cosmetic"@en ;
                                 rdfs:comment "" ;
                                 owl:priorVersion "3.0"@en ;
                                 owl:versionInfo "4.0"@en .

