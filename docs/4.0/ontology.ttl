#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AfterfeelingState
                :Chemical_nature
                :Dosage
                :EmollienceState
                :Formulation
                :Heuristic
                :Ingredient
                :IonicState
                :Origin
                :SpreadingState
                :TextureState
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :HeterogenousMixture
                :HomogenousMixture
                :OneIngredient
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :NonStable
                        :Stable
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :gas
                        :liquid
                      )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :gas
                        :solid
                      )
