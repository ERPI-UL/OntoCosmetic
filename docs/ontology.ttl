@prefix : <https://purl.org/ontocosmetic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://purl.org/ontocosmetic#> .

<https://purl.org/ontocosmetic> rdf:type owl:Ontology ;
                                 rdfs:comment "An ontology that describes chemical formulation in the perspective to assist the design process of cosmetic product."@en ;
                                 rdfs:label "Onto Cosmetic"@en ;
                                 owl:priorVersion "2.0"@en ;
                                 owl:versionInfo "3.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/contributor
<http://purl.org/dc/elements/1.1/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://purl.org/ontocosmetic#hasAfterfeeling
:hasAfterfeeling rdf:type owl:ObjectProperty ;
                 rdfs:domain :Emollient ;
                 rdfs:range :Afterfeeling ;
                 rdfs:isDefinedBy "Give one or several afterfeeling properties to an ingredient of type emollient."@en .


###  https://purl.org/ontocosmetic#hasChemicalNature
:hasChemicalNature rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Ingredient ;
                   rdfs:range :Chemical_nature ;
                   rdfs:isDefinedBy "Give the chemical nature of an ingredient."@en .


###  https://purl.org/ontocosmetic#hasDosage
:hasDosage rdf:type owl:ObjectProperty ;
           rdfs:domain :Formulation ;
           rdfs:range :Dosage ;
           rdfs:isDefinedBy "Give the dosages that compose a formulation."@en .


###  https://purl.org/ontocosmetic#hasDosageIngredient
:hasDosageIngredient rdf:type owl:ObjectProperty ;
                     rdfs:domain :Dosage ;
                     rdfs:range :Ingredient ;
                     rdfs:isDefinedBy "Give the ingredient of a dosage."@en .


###  https://purl.org/ontocosmetic#hasEmollience
:hasEmollience rdf:type owl:ObjectProperty ;
               rdfs:domain :Emollient ;
               rdfs:range :Emollience ;
               rdfs:isDefinedBy "Give the level of emollience of an ingredient of type emollient."@en .


###  https://purl.org/ontocosmetic#hasHeuristicIngProp
:hasHeuristicIngProp rdf:type owl:ObjectProperty ;
                     rdfs:domain :Heuristic ;
                     rdfs:range :IngredientProperty ;
                     rdfs:isDefinedBy "Associate an heuristic to the ingredient property that is involved in the rule."@en .


###  https://purl.org/ontocosmetic#hasHeuristicIngType
:hasHeuristicIngType rdf:type owl:ObjectProperty ;
                     rdfs:domain :Heuristic ;
                     rdfs:range :Ingredient_Type ;
                     rdfs:isDefinedBy "Define which ingredient type is concerned by the heuristic."@en .


###  https://purl.org/ontocosmetic#hasHeuristicProcessOutcome
:hasHeuristicProcessOutcome rdf:type owl:ObjectProperty ;
                            rdfs:domain :Heuristic ;
                            rdfs:range :DropletSize ;
                            rdfs:isDefinedBy "Associate an heuristic to the structure property of the formulation final product  that is involved in the rule."@en .


###  https://purl.org/ontocosmetic#hasHeuristicProcessProp
:hasHeuristicProcessProp rdf:type owl:ObjectProperty ;
                         rdfs:domain :Heuristic ;
                         rdfs:range :Process ;
                         rdfs:isDefinedBy "Associate an heuristic to the process used to do the formulation final product. This process will have an impact on the product structure or properties."@en .


###  https://purl.org/ontocosmetic#hasHeuristicProdProp
:hasHeuristicProdProp rdf:type owl:ObjectProperty ;
                      rdfs:domain :Heuristic ;
                      rdfs:range :ProductProperty ;
                      rdfs:isDefinedBy "Associate an heuristic to a product property. The rule has an impact on the properties of the final product formulated."@en .


###  https://purl.org/ontocosmetic#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:domain :Formulation ;
               rdfs:range :Ingredient .


###  https://purl.org/ontocosmetic#hasIonicClass
:hasIonicClass rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Surfactant ;
               rdfs:range :Ionic_Nature ;
               rdfs:isDefinedBy "Define the ionic nature of an ingredient."@en .


###  https://purl.org/ontocosmetic#hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
           rdfs:domain :Ingredient ;
           rdfs:range :Origin ;
           rdfs:isDefinedBy "Provide the origin of an ingredient"@en .


###  https://purl.org/ontocosmetic#hasPhase
:hasPhase rdf:type owl:ObjectProperty ;
          rdfs:domain :Ingredient ;
          rdfs:range :Phase ;
          rdfs:isDefinedBy "it define which phase the ingredient is soluble in."@en .


###  https://purl.org/ontocosmetic#hasProcessing
:hasProcessing rdf:type owl:ObjectProperty ;
               rdfs:domain :Formulation ;
               rdfs:range :Process ;
               rdfs:isDefinedBy "Processing done on the ingredient of the formulation."@en .


###  https://purl.org/ontocosmetic#hasProductDropletSize
:hasProductDropletSize rdf:type owl:ObjectProperty ;
                       rdfs:domain :Formulation ;
                       rdfs:range :DropletSize ;
                       rdfs:isDefinedBy "Provides the size of the droplet of the final product of a formulation. This dorplet will depends on process."@en .


###  https://purl.org/ontocosmetic#hasProductOiliness
:hasProductOiliness rdf:type owl:ObjectProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range :Oiliness ;
                    rdfs:isDefinedBy "Define the oiliness of formulated product."@en .


###  https://purl.org/ontocosmetic#hasProductStability
:hasProductStability rdf:type owl:ObjectProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range :Stability ;
                     rdfs:isDefinedBy "Define the stability of a fomulation final product."@en .


###  https://purl.org/ontocosmetic#hasProductViscosity
:hasProductViscosity rdf:type owl:ObjectProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range :Viscosity ;
                     rdfs:isDefinedBy "Provides the viscosity of the formulation final product."@en .


###  https://purl.org/ontocosmetic#hasRecommendationHeuristic
:hasRecommendationHeuristic rdf:type owl:ObjectProperty ;
                            rdfs:domain :Formulation ;
                            rdfs:range :Heuristic ;
                            rdfs:isDefinedBy "Provide the heuristic that will be applied to a formulation as recommendation herustic."@en .


###  https://purl.org/ontocosmetic#hasRestrictiveHeuristic
:hasRestrictiveHeuristic rdf:type owl:ObjectProperty ;
                         rdfs:domain :Formulation ;
                         rdfs:range :Heuristic .


###  https://purl.org/ontocosmetic#hasSpreading
:hasSpreading rdf:type owl:ObjectProperty ;
              rdfs:domain :Emollient ;
              rdfs:range :Spreading ;
              rdfs:isDefinedBy "Defines the spreading property of an emollient."@en .


###  https://purl.org/ontocosmetic#hasState
:hasState rdf:type owl:ObjectProperty ;
          rdfs:domain :Substance ;
          rdfs:range :StateOfMatter .


###  https://purl.org/ontocosmetic#hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            rdfs:domain :Surfactant ;
            rdfs:range :Texture ;
            rdfs:isDefinedBy "Provides the texture that can be expected from a given surfactant."@en .


###  https://purl.org/ontocosmetic#isCommercializedAs
:isCommercializedAs rdf:type owl:ObjectProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range :Product .


###  https://purl.org/ontocosmetic#isValidatedBy
:isValidatedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :Formulation ;
               rdfs:range :Heuristic ;
               rdfs:isDefinedBy "Defines that a formulation validate a given heuristic."@en .


#################################################################
#    Data properties
#################################################################

###  https://purl.org/ontocosmetic#hasCalculatedHLB
:hasCalculatedHLB rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Formulation ;
                  rdfs:range xsd:float ;
                  rdfs:comment "the final HLB of the surfactant mixture that compose the formulation"@en .


###  https://purl.org/ontocosmetic#hasEquivalentHLB
:hasEquivalentHLB rdf:type owl:DatatypeProperty .


###  https://purl.org/ontocosmetic#hasHLB
:hasHLB rdf:type owl:DatatypeProperty ;
        rdfs:domain :Surfactant ;
        rdfs:comment "the HLB that a surfactant can have"@en .


###  https://purl.org/ontocosmetic#hasHeuristicDescription
:hasHeuristicDescription rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Heuristic ;
                         rdfs:isDefinedBy "Data properties that provides a description of the heuristic."@en .


###  https://purl.org/ontocosmetic#hasHeuristicHighThreshold
:hasHeuristicHighThreshold rdf:type owl:DatatypeProperty ;
                           rdfs:domain :Heuristic ;
                           rdfs:range xsd:float ;
                           rdfs:isDefinedBy "High threshold in percentage for an heuristique."@en .


###  https://purl.org/ontocosmetic#hasHeuristicLowThreshold
:hasHeuristicLowThreshold rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Heuristic ;
                          rdfs:range xsd:float ;
                          rdfs:isDefinedBy "Low threshold in percentage for an heuristique."@en .


###  https://purl.org/ontocosmetic#hasHeuristicSource
:hasHeuristicSource rdf:type owl:DatatypeProperty ;
