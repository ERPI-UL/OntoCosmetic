                    rdfs:isDefinedBy "Provides the source of the heuristic (from literature or experience)"@en .


###  https://purl.org/ontocosmetic#hasINCIcode
:hasINCIcode rdf:type owl:DatatypeProperty ;
             rdfs:domain :Ingredient ;
             rdfs:range xsd:string ;
             rdfs:comment "the INCI code that a ingredient can have."@en .


###  https://purl.org/ontocosmetic#hasNbSurfactantIng
:hasNbSurfactantIng rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Formulation ;
                    rdfs:range xsd:int ;
                    rdfs:isDefinedBy "Define the number of surfactants inside a given formulation."@en .


###  https://purl.org/ontocosmetic#hasPricePerKilogram
:hasPricePerKilogram rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Ingredient ;
                     rdfs:range xsd:float ;
                     rdfs:comment "Price per kilogram of an ingredient"@en .


###  https://purl.org/ontocosmetic#hasProductOilyPhase
:hasProductOilyPhase rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Formulation ;
                     rdfs:range xsd:float ;
                     rdfs:isDefinedBy "Percentage of the formulated product that represent oily ingrdient."@en .


###  https://purl.org/ontocosmetic#hasQuantity
:hasQuantity rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Dosage ;
             rdfs:range xsd:float ;
             rdfs:isDefinedBy "the quantity of ingredient that compose the dosage. This quantity is in percentage of volume of the formulation."@en .


###  https://purl.org/ontocosmetic#hasQuantitySum
:hasQuantitySum rdf:type owl:DatatypeProperty ;
                rdfs:domain :Dosage ;
                rdfs:range xsd:float ;
                rdfs:comment "the quantity sum is an intermediary value that allow the calculation of the total quantity of product in a formulation, except the water. This quantity sum will be used in the calculation of the water quantity."@en .


###  https://purl.org/ontocosmetic#hasRequiredHLB
:hasRequiredHLB rdf:type owl:DatatypeProperty ;
                rdfs:domain :Emollient ;
                rdfs:comment "the RHLB that an emollient can have."@en .


###  https://purl.org/ontocosmetic#hasSurfactantQuantitySum
:hasSurfactantQuantitySum rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Dosage ;
                          rdfs:range xsd:float ;
                          rdfs:comment "The surfactant quantity sum is an intermediary value that allow the calculation of the total quantity of surfactant in a formulation."@en .


###  https://purl.org/ontocosmetic#hasTotalOilyPhase
:hasTotalOilyPhase rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Formulation ;
                   rdfs:range xsd:float .


###  https://purl.org/ontocosmetic#hasTotalSurfactantQuantity
:hasTotalSurfactantQuantity rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain :Formulation ;
                            rdfs:range xsd:float ;
                            rdfs:comment "The total quantity of surfactant that a formulation have. This quantity is in percentage of volume."@en .


###  https://purl.org/ontocosmetic#hasTotalThickenerQuantity
:hasTotalThickenerQuantity rdf:type owl:DatatypeProperty ;
                           rdfs:domain :Formulation ;
                           rdfs:range xsd:float ;
                           rdfs:isDefinedBy "The total quantity of thickener that a formulation have. This quantity is in percentage of volume."@en .


###  https://purl.org/ontocosmetic#isActive
:isActive rdf:type owl:DatatypeProperty ;
          rdfs:domain :Heuristic ;
          rdfs:range xsd:boolean ;
          rdfs:isDefinedBy "Property that allow to enable/ disable a heuristic"@en .


###  https://purl.org/ontocosmetic#isConform
:isConform rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Formulation ;
           rdfs:range xsd:boolean ;
           rdfs:comment "the data property idicates if a formulation is conform to the restricive heuristics that correspond to formulation specification."@en .


#################################################################
#    Classes
#################################################################

###  https://purl.org/ontocosmetic#AbsorptionRate
:AbsorptionRate rdf:type owl:Class ;
                rdfs:subClassOf :ProductProperty ;
                rdfs:isDefinedBy "Describe the capacity of the formulation product to be absopt by a skin."@en ;
                rdfs:label "After Feel - Absorption Rate"@en .


###  https://purl.org/ontocosmetic#Active
:Active rdf:type owl:Class ;
        rdfs:subClassOf :Ingredient ;
        rdfs:isDefinedBy "Ingredients that perform an specific function on customer skin. [cosmetic sector definition]. It is a chemical that helps with the main function of the cosmetic product."@en .


###  https://purl.org/ontocosmetic#Aerosol
:Aerosol rdf:type owl:Class ;
         rdfs:subClassOf :Colloid ;
         rdfs:isDefinedBy "solid or liquid particles in a gas"@en .


###  https://purl.org/ontocosmetic#Afterfeeling
:Afterfeeling rdf:type owl:Class ;
              rdfs:subClassOf :IngredientProperty ;
              rdfs:isDefinedBy "Ingredients have the property to influence the afterfeel of the formulated product."@en .


###  https://purl.org/ontocosmetic#Agitation
:Agitation rdf:type owl:Class ;
           rdfs:subClassOf :Process .


###  https://purl.org/ontocosmetic#AmphotericSurfactant
:AmphotericSurfactant rdf:type owl:Class ;
                      rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#AnionicSurfactant
:AnionicSurfactant rdf:type owl:Class ;
                   rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#AnionicThickener
:AnionicThickener rdf:type owl:Class ;
                  rdfs:subClassOf :IonicThickener .


###  https://purl.org/ontocosmetic#Antioxidant
:Antioxidant rdf:type owl:Class ;
             rdfs:subClassOf :Active ;
             rdfs:isDefinedBy "Chemicals that interrupt oxidation reactions and prevent the effects of oxygen radicals (e.g. peroxides) both processes known to damage the integrity and function of various natural substances. Antioxidants are useful in two ways: On the one hand they prevent degradation of natural ingredients (proteins, sugars, lipids) in the cosmetic product. On the other hand antioxidants protect the skin cells from being damaged and slow down the aging process. Antioxidants have been shown to boost the skin's radiance, minimize age spots, sun spots, and fine lines."@en .


###  https://purl.org/ontocosmetic#AqueousThickeners
:AqueousThickeners rdf:type owl:Class ;
                   rdfs:subClassOf :Thickener .


###  https://purl.org/ontocosmetic#CationicSurfactant
:CationicSurfactant rdf:type owl:Class ;
                    rdfs:subClassOf :IonicSurfactant .


###  https://purl.org/ontocosmetic#CationicThickener
:CationicThickener rdf:type owl:Class ;
                   rdfs:subClassOf :IonicThickener .


###  https://purl.org/ontocosmetic#Characteristic
:Characteristic rdf:type owl:Class .


###  https://purl.org/ontocosmetic#Chemical_nature
:Chemical_nature rdf:type owl:Class ;
                 rdfs:subClassOf :IngredientProperty .


###  https://purl.org/ontocosmetic#Colloid
:Colloid rdf:type owl:Class ;
         rdfs:subClassOf :HeterogenousMixture ;
         rdfs:isDefinedBy "It is a heterogeneous mixture in which a substance with a microscopic structure of disperse insoluble particles (disperse phase) is suspended in another substance (continuous phase). It can exist naturally, or it can be technically formulated, and in that case, it can also be named as a Formulation."@en ;
         rdfs:seeAlso "colloidal system"@en .


###  https://purl.org/ontocosmetic#DegreeOfGenerality
:DegreeOfGenerality rdf:type owl:Class ;
                    rdfs:subClassOf :Heuristic .


###  https://purl.org/ontocosmetic#Density
:Density rdf:type owl:Class ;
         rdfs:subClassOf :EmollientProperty ;
         rdfs:isDefinedBy "It is the mass per unit of volume. In emulsions it is related to emulsion stability."@en .


###  https://purl.org/ontocosmetic#Descriptors
:Descriptors rdf:type owl:Class ;
             rdfs:subClassOf :Characteristic .


###  https://purl.org/ontocosmetic#Dosage
:Dosage rdf:type owl:Class ;
        rdfs:comment "Dosages are the combination of a substance and a quantity. It constitute a part of a formulation."@en .


###  https://purl.org/ontocosmetic#DropletSize
:DropletSize rdf:type owl:Class ;
             rdfs:subClassOf :ProductProperty ;
             rdfs:isDefinedBy "Fromulated product can be emulsions. These emulsions are caracterized by the size of the droplets"@en .


###  https://purl.org/ontocosmetic#Efficiency
:Efficiency rdf:type owl:Class ;
            rdfs:subClassOf :ThickenerProperty ;
            rdfs:isDefinedBy "It is the ability to increase viscosity using a certain quantity of the rheological modifier. Efficiency depends on the polymer nature, molecular weight, branches, and conformation."@en .


###  https://purl.org/ontocosmetic#ElectricalNature
:ElectricalNature rdf:type owl:Class ;
                  rdfs:subClassOf :SurfactantProperty .


###  https://purl.org/ontocosmetic#Emollience
:Emollience rdf:type owl:Class ;
            rdfs:subClassOf :IngredientProperty .


###  https://purl.org/ontocosmetic#Emollient
:Emollient rdf:type owl:Class ;
           rdfs:subClassOf :Ingredient ;
           rdfs:isDefinedBy "Ingredient of oily nature used in cosmetics to make skin and hair softener. Some of them can serve as solvent for liposoluble actives. Additionally, some of them can act as occlusive ingredients, i.e., they create a barrier to avoid the loss of water. It can be classified according to its chemical nature into the sub-classes:  ester, fatty alcohol, fatty acid, hydrocarbon, silicon, triglyceride."@en .


###  https://purl.org/ontocosmetic#EmollientProperty
:EmollientProperty rdf:type owl:Class ;
                   rdfs:subClassOf :Property .


###  https://purl.org/ontocosmetic#Emollient_Dosage
:Emollient_Dosage rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Dosage
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasDosageIngredient ;
                                                               owl:someValuesFrom :Emollient
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Dosage ;
                  owl:disjointWith :Surfactant_Dosage ;
                  rdfs:isDefinedBy "sub-type of dosage which concerns only the emollient."@en .


###  https://purl.org/ontocosmetic#Emulsion
:Emulsion rdf:type owl:Class ;
          rdfs:subClassOf :Colloid ;
          rdfs:isDefinedBy "It is a heterogeneous mixture of two or more immiscible liquids. It is a sub-class of a more general class known as colloid."@en .


###  https://purl.org/ontocosmetic#EmulsionScienceHeuristic
:EmulsionScienceHeuristic rdf:type owl:Class ;
                          rdfs:subClassOf :Heuristic ;
                          rdfs:isDefinedBy "It is a sub-type of heuristic that come from scientific literature."@en .


###  https://purl.org/ontocosmetic#Ester_emollient
:Ester_emollient rdf:type owl:Class ;
                 rdfs:subClassOf :Emollient ;
                 rdfs:isDefinedBy "chemical compound derived from an oxoacid (organic or inorganic) in which at least one –OH hydroxyl group is replaced by an –O– alkyl (alkoxy) group."@en .


###  https://purl.org/ontocosmetic#Ether_emollient
:Ether_emollient rdf:type owl:Class ;
                 rdfs:subClassOf :Emollient ;
                 rdfs:isDefinedBy "Sub type of emollient ingredient that are ether."@en .


###  https://purl.org/ontocosmetic#ExpertKnowledgeHeuristic
:ExpertKnowledgeHeuristic rdf:type owl:Class ;
                          rdfs:subClassOf :Heuristic ;
                          rdfs:isDefinedBy "It is a sub-type of heuristic that come from expert  interview."@en .


###  https://purl.org/ontocosmetic#Fatty_alcohol_emollient
:Fatty_alcohol_emollient rdf:type owl:Class ;
                         rdfs:subClassOf :Emollient ;
                         rdfs:isDefinedBy "Sub type of emollient ingredient that are fatty alcohol."@en .


###  https://purl.org/ontocosmetic#Foam
:Foam rdf:type owl:Class ;
      rdfs:subClassOf :Colloid ;
      rdfs:isDefinedBy "gas in liquid."@en .


###  https://purl.org/ontocosmetic#Formulation
:Formulation rdf:type owl:Class ;
             rdfs:isDefinedBy "Designed mixture of ingredients combined with an appropriated process. It can potentially take any substance form. It is a technical created object. It can have different forms: solid, liquid, gas. And it can be homogeneous (solutions) or heterogeneous (colloidal system). It is a subclass of substance and a super class of solution, mixture, emulsion, colloid."@en ;
             rdfs:seeAlso :Mixture ,
                          "Blend"@en .


###  https://purl.org/ontocosmetic#Gel
:Gel rdf:type owl:Class ;
     rdfs:subClassOf :Colloid ;
     rdfs:isDefinedBy "liquids in solids."@en .


###  https://purl.org/ontocosmetic#Glycols
:Glycols rdf:type owl:Class ;
         rdfs:subClassOf :Humectant ;
         rdfs:isDefinedBy "They are chemical compounds containing a certain number of hydroxyl groups. They are water- soluble and are widely used in cosmetics due to their moisturizing function. They also work as stabilizer solvents and dispersants of polymers. They have a significant effect on the feel of use."@en .


###  https://purl.org/ontocosmetic#HLB
:HLB rdf:type owl:Class ;
     rdfs:subClassOf :SurfactantProperty .


###  https://purl.org/ontocosmetic#HeterogenousMixture
:HeterogenousMixture rdf:type owl:Class ;
                     rdfs:subClassOf :Mixture ;
                     rdfs:isDefinedBy "It is a mixture in which the composition is not uniform throughout the mixture. It two or more phases in contact."@en ;
                     rdfs:label "Heterogenous Mixture"@en .


###  https://purl.org/ontocosmetic#Heuristic
:Heuristic rdf:type owl:Class ;
           rdfs:isDefinedBy "Rules that are used by product formulator to anticipate what happens while combined different ingredients."@en .


###  https://purl.org/ontocosmetic#HeuristicBodyCream
:HeuristicBodyCream rdf:type owl:Class ;
                    rdfs:subClassOf :SpecificApplication .


###  https://purl.org/ontocosmetic#HeuristicFacialCream
:HeuristicFacialCream rdf:type owl:Class ;
                      rdfs:subClassOf :SpecificApplication .


###  https://purl.org/ontocosmetic#HeuristicForActive
:HeuristicForActive rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Heuristic
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasHeuristicIngType ;
                                                                 owl:hasValue :active
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Heuristic ;
                    rdfs:isDefinedBy "Sub group of heuristic that only concerns active ingredients."@en .


###  https://purl.org/ontocosmetic#HeuristicForEmollient
:HeuristicForEmollient rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Heuristic
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasHeuristicIngType ;
                                                                    owl:hasValue :emollient
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :Heuristic ;
