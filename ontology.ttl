:NonConformFormulation rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Formulation
                                                                  [ rdf:type owl:Class ;
                                                                    owl:complementOf :ConformFormulation
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ,
                                           [ owl:intersectionOf ( :Formulation
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :isConform ;
                                                                    owl:hasValue "false"^^xsd:boolean
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :Formulation ;
                       rdfs:comment "The formulation is not conform to the specification defined for the final product. It means the formulation have at least one specification which is not satisfied."@en .


